{
    "version": 2,
    "builds": [
      {
        "src": "src/backend/server/server.ts",
        "use": "@vercel/node@1.8.1",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "src/index.ts"
      }
    ]
  }
  